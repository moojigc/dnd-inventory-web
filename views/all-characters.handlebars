{{> navbar}}

<main class="container-lg wrapper">
    <header>
        <h1>All Characters</h1>
    </header>
    <div class="row">
        <div class="col-8" style="margin-left: auto; margin-right: auto">
            <div class="card">
                <div class="card-body">
                    <ul class="list-group">
                        <li class="list-group-item">Subaru</li>
                        {{#each character}}
                        <li class="list-group-item">{{name}}</li>
                        {{/each}}
                    </ul>
                </div>
            </div>
        </div>
    </div>
</main>